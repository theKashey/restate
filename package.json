{
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "changelog": "conventional-changelog -p angular -r2 && conventional-github-releaser -p angular",
    "dev": "yarn bootstrap && yarn build && lerna-watch",
    "build": "lerna-build",
    "format": "prettier --write \"**/*.{js,md,ts,json}\" *.{js,md,ts,json}",
    "lint": "eslint . --fix",
    "prepublishOnly": "yarn build",
    "prebuild": "rm -rf packages/*/lib",
    "precommit": "lint-staged",
    "test": "jest --no-cache",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,md,ts,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.5.0",
    "conventional-changelog": "^2.0.0",
    "conventional-github-releaser": "^2.0.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0",
    "lerna": "^2.5.1",
    "lerna-tools": "^1.0.0",
    "lint-staged": "^7.1.2",
    "prettier": "^1.9.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.4.0",
    "@storybook/react": "^3.3.12",
    "@storybook/addon-actions": "^3.3.12",
    "@storybook/addon-links": "^3.3.12"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "setupFiles": [
      "<rootDir>/__tests/setupTests.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/__tests/babel.js"
    }
  },
  "dependencies": {
    "babel-jest": "^23.0.0",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "react": "^16.4.0",
    "storybook": "^1.0.0"
  }
